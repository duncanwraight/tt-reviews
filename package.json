{
  "name": "tt-reviews",
  "version": "1.0.0",
  "description": "A trusted table tennis equipment review platform featuring player profiles and community-moderated reviews.",
  "main": "index.js",
  "type": "module",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "dev": "concurrently \"npm run dev:tsc\" \"npm run dev:wrangler\"",
    "dev:tsc": "tsc --watch",
    "dev:wrangler": "wrangler dev --env dev",
    "dev:legacy": "wrangler dev --env dev",
    "deploy": "wrangler deploy",
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:discord": "vitest run src/test/discord-simple.test.ts src/controllers/discord.controller.test.ts",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "check": "npm run format:check && npm run lint && npm run typecheck && npm run test && npm run build",
    "check:discord": "npm run format:check && npm run lint && npm run typecheck && npm run test:discord && npm run build",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/duncanwraight/tt-reviews.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/duncanwraight/tt-reviews/issues"
  },
  "homepage": "https://github.com/duncanwraight/tt-reviews#readme",
  "dependencies": {
    "@supabase/supabase-js": "^2.50.0",
    "hono": "^4.7.11"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250607.0",
    "@types/node": "^22.15.30",
    "@typescript-eslint/eslint-plugin": "^8.33.1",
    "@typescript-eslint/parser": "^8.33.1",
    "@vitest/ui": "^3.2.3",
    "concurrently": "^9.1.2",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.1",
    "husky": "^9.0.11",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3",
    "vitest": "^3.2.3",
    "wrangler": "^4.19.1"
  }
}
